.banner {
    width: 100%;
    height: 750rpx;
    position: relative;
}

.swiper {
    width: 100%;
    height: 750rpx;
    text-align: center;
}

.monitor {
    width: 120rpx;
    height: 120rpx;
    position: absolute;
    top: 10rpx;
    right: 20rpx;
}

.video {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.slide-image {
    width: 100%;
    height: 750rpx;
}

.block20 {
    height: 20rpx;
    background-color: #f4f4f4;
}

.title {
    width: 100%;
    text-align: center;
    font-size: 32rpx;
    padding: 30rpx 0rpx;
    color: #3d3d3d;
}

.adoptState {
    font-size: 30rpx;
    text-align: center;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    color: #8b8b8b;
}

.timeAxis {
    width: 100%;
    height: 500rpx;
    display: flex;
}

.recodeTime {
    width: 35%;
    height: 100%;
    border-right: 5rpx solid #1795b7;
    position: relative;
}

.recodeContent {
    width: 65%;
    height: 100%;
    position: relative;
}

.reTime {
    position: relative;
    width: 100%;
    height: 100%;
}

.tiems {
    font-size: 24rpx;
    background-color: #1795b7;
    color: #fff;
    width: 220rpx;
    height: 45rpx;
    text-align: center;
    line-height: 45rpx;
    position: relative;
    border-radius: 5rpx;
    top: 0rpx;
    right: -10rpx;
}

.tiems:after {
    content: "";
    width: 0;
    height: 0;
    right: -28rpx;
    bottom: 6rpx;
    position: absolute;
    border-width: 7px;
    border-style: solid;
    border-color: transparent transparent transparent #1795b7;
}

.circle {
    width: 30rpx;
    height: 30rpx;
    background-color: #1795b7;
    border-radius: 50%;
    position: absolute;
    right: -15rpx;
    top: -10rpx;
}

.reContent {
    width: 420rpx;
    border-radius: 10rpx;
    background-color: #2dacb9;
    font-size: 30rpx;
    color: #fff;
    position: absolute;
    padding: 10rpx;
    box-sizing: border-box;
    top: 0rpx;
    left: 40rpx;
}

.reContent:after {
    content: "";
    width: 0;
    height: 0;
    left: -40rpx;
    top: 20rpx;
    position: absolute;
    border-width: 10px;
    border-style: solid;
    border-color: transparent #2dacb9 transparent transparent;
}

.recodeImg {
    width: 180rpx;
    height: 180rpx;
}

.recodeBImg {
    width: 300rpx;
    height: 300rpx;
}

.Slaughter {
    width: 100%;
    border: none;
    background-color: #fc8535;
    color: #fff;
    border-radius: 0rpx;
    margin-top: 10rpx;
    position: fixed;
    bottom: 0;
}

.Slaughter::after {
    border: none;
}

.footer_block {
    height: 110rpx;
}

.footer_view {
    width: 100%;
    height: 100rpx;
    color: #fff;
    background: #FC8535;
    line-height: 100rpx;
    letter-spacing: 5rpx;
    font-size: 34prpx;
    text-align: center;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 1100;
}

.model {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.4);
    z-index: 1200;
}

.notice_container {
    position: fixed;
    width: 80%;
    background-color: #fff;
    border-radius: 20rpx;
    left: 10%;
    top: 250rpx;
    z-index: 1300;
    overflow: hidden;
    transition: all .3s;
}

.scale0 {
    transform: scale(0,0);
}

.scale1 {
    transform: scale(1,1);
}

.recovery_container {
    text-align: center;
    margin-top: 80rpx;
}

.recovery_img {
    width: 220rpx;
    height: 220rpx;
}

.remind_title {
    font-size: 34rpx;
    font-weight: 600rpx;
    text-align: center;
    margin-top: 30rpx;
    letter-spacing: 3rpx;
}

.inner_container {
    font-size: 28rpx;
    color: #9C9C9C;
    text-align: center;
    letter-spacing: 3rpx;
}

.footer_btn {
    border-top: 1rpx solid #E1E1E1;
    height: 100rpx;
    line-height: 100rpx;
    margin-top: 40rpx;
    display: flex;
    align-items: center;
    color: #6D6D6D;
}

.footer_btn view {
    width: 50%;
    height: 100%;
    text-align: center;
    font-size: 32rpx;
    letter-spacing: 5rpx;
}

.confrim {
    border-left: 1rpx solid #E1E1E1;
    color: #fc8535;
}