.iconWrapper {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    width: 750rpx;
    height: auto;
}

.swiper_item {
    width: 100%;
    height: 100%;
    display: flex;
    flex-wrap: wrap;
}

.icon1 {
    width: 50%;
    height: 200rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.icon1 .icon_content {
    width: 250rpx;
    text-align: center;
}

.icon1 image {
    width: 150rpx;
    height: 150rpx;
}

